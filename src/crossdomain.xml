<?xml version="1.0"?>
<!DOCTYPE cross-domain-policy SYSTEM "http://www.adobe.com/xml/dtds/cross-domain-policy.dtd">
<cross-domain-policy>
	<site-control permitted-cross-domain-policies="master-only"/>
	<allow-access-from domain="app.roxee.net" to-ports="80" secure="true"/>
	<allow-http-request-headers-from domain="app.roxee.net" headers="*" secure="true"/>
</cross-domain-policy>
